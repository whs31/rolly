        </templateparamlist>
        <type>concept</type>
        <definition>concept timed_lockable</definition>
        <argsstring></argsstring>
        <name>timed_lockable</name>
        <initializer>= <ref refid="namespacerolly_1_1concepts_1a7a511e60ad4941089b08e074792f3225" kindref="member">lockable</ref>&lt;T&gt;
